    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "declaration": true,
    "outDir": "bin",
    "rootDir": ".",
    "strict": true,
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  }
}